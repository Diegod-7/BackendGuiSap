{
  "steps": {
    "startTransaction": {
      "action": "callProgram",
      "method": "sapSession.StartTransaction",
      "paramKey": "Tcode",
      "next": "waitVariant"
    },
    "waitVariant": {
      "action": "waitFor",
      "target": "variant.window",
      "timeout": "30s",
      "next": "setDatabase"
    },
    "setDatabase": {
      "action": "set",
      "target": "mb51.filter.database",
      "paramKey": "Database",
      "next": "setShortDocuments"
    },
    "setShortDocuments": {
      "action": "set",
      "target": "mb51.filter.shortDocuments",
      "paramKey": "ShortDocuments",
      "next": "setRereadArchive"
    },
    "setRereadArchive": {
      "action": "set",
      "target": "mb51.filter.rereadShortDocsInArchive",
      "paramKey": "RereadShortDocsInArchive",
      "next": "setAlvDef"
    },
    "setAlvDef": {
      "action": "set",
      "target": "mb51.filter.alvDef",
      "paramKey": "ALV_DEF",
      "next": "setPaAistr"
    },
    "setPaAistr": {
      "action": "set",
      "target": "mb51.filter.paAistr",
      "paramKey": "PA_AISTR",
      "next": "setMaterialLow"
    },
    "setMaterialLow": {
      "action": "set",
      "target": "mb51.filter.materialLow",
      "paramKey": "MaterialLow",
      "next": "setMaterialHigh"
    },
    "setMaterialHigh": {
      "action": "set",
      "target": "mb51.filter.materialHigh",
      "paramKey": "MaterialHigh",
      "next": "setPlantLow"
    },
    "setPlantLow": {
      "action": "set",
      "target": "mb51.filter.plantLow",
      "paramKey": "PlantLow",
      "next": "setPlantHigh"
    },
    "setPlantHigh": {
      "action": "set",
      "target": "mb51.filter.plantHigh",
      "paramKey": "PlantHigh",
      "next": "setStorageLocationLow"
    },
    "setStorageLocationLow": {
      "action": "set",
      "target": "mb51.filter.storageLocationLow",
      "paramKey": "StorageLocationLow",
      "next": "setStorageLocationHigh"
    },
    "setStorageLocationHigh": {
      "action": "set",
      "target": "mb51.filter.storageLocationHigh",
      "paramKey": "StorageLocationHigh",
      "next": "setBatchLow"
    },
    "setBatchLow": {
      "action": "set",
      "target": "mb51.filter.batchLow",
      "paramKey": "BatchLow",
      "next": "setBatchHigh"
    },
    "setBatchHigh": {
      "action": "set",
      "target": "mb51.filter.batchHigh",
      "paramKey": "BatchHigh",
      "next": "setVendorLow"
    },
    "setVendorLow": {
      "action": "set",
      "target": "mb51.filter.vendorLow",
      "paramKey": "VendorLow",
      "next": "setVendorHigh"
    },
    "setVendorHigh": {
      "action": "set",
      "target": "mb51.filter.vendorHigh",
      "paramKey": "VendorHigh",
      "next": "setCustomerLow"
    },
    "setCustomerLow": {
      "action": "set",
      "target": "mb51.filter.customerLow",
      "paramKey": "CustomerLow",
      "next": "setCustomerHigh"
    },
    "setCustomerHigh": {
      "action": "set",
      "target": "mb51.filter.customerHigh",
      "paramKey": "CustomerHigh",
      "next": "setMovementTypeLow"
    },
    "setMovementTypeLow": {
      "action": "set",
      "target": "mb51.filter.movementTypeLow",
      "paramKey": "MovementTypeLow",
      "next": "setMovementTypeHigh"
    },
    "setMovementTypeHigh": {
      "action": "set",
      "target": "mb51.filter.movementTypeHigh",
      "paramKey": "MovementTypeHigh",
      "next": "setSpecialStockLow"
    },
    "setSpecialStockLow": {
      "action": "set",
      "target": "mb51.filter.specialStockLow",
      "paramKey": "SpecialStockLow",
      "next": "setSpecialStockHigh"
    },
    "setSpecialStockHigh": {
      "action": "set",
      "target": "mb51.filter.specialStockHigh",
      "paramKey": "SpecialStockHigh",
      "next": "setTransEventTypeLow"
    },
    "setTransEventTypeLow": {
      "action": "set",
      "target": "mb51.filter.transEventTypeLow",
      "paramKey": "TransEventTypeLow",
      "next": "setTransEventTypeHigh"
    },
    "setTransEventTypeHigh": {
      "action": "set",
      "target": "mb51.filter.transEventTypeHigh",
      "paramKey": "TransEventTypeHigh",
      "next": "clickExecute"
    },
    "clickExecute": {
      "action": "click",
      "target": "variant.execBtn",
      "next": "waitProcessing"
    },
    "waitProcessing": {
      "action": "waitFor",
      "target": "processing",
      "timeout": "60s",
      "next": "checkError"
    },
    "checkError": {
      "action": "condition",
      "target": "errorDialog",
      "operator": "exists",
      "true": "notifyErr",
      "false": "handleLayout"
    },
    "notifyErr": {
      "action": "notify",
      "paramKey": "ErrorMessage",
      "next": "end"
    },
    "handleLayout": {
      "action": "condition",
      "paramKey": "HasLayout",
      "operator": "==",
      "value": true,
      "true": "chooseLayout",
      "false": "handleColumns"
    },
    "chooseLayout": {
      "action": "click",
      "target": "chooseLayout.btn",
      "next": "setLayoutName"
    },
    "setLayoutName": {
      "action": "set",
      "target": "chooseLayout.table",
      "paramKey": "LayoutName",
      "next": "confirmLayout"
    },
    "confirmLayout": {
      "action": "click",
      "target": "chooseLayout.confirm",
      "next": "handleColumns"
    },
    "handleColumns": {
      "action": "condition",
      "paramKey": "Columns",
      "operator": "exists",
      "true": "changeLayout",
      "false": "download"
    },
    "changeLayout": {
      "action": "click",
      "target": "changeLayout.btn",
      "next": "adoptLayout"
    },
    "adoptLayout": {
      "action": "click",
      "target": "changeLayout.adopt",
      "next": "download"
    },
    "download": {
      "action": "click",
      "target": "saveAs.btn",
      "next": "end"
    },
    "end": {
      "action": "exit"
    }
  }
}