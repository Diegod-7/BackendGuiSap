{
  "steps": {
    "startTransaction": {
      "action": "callProgram",
      "method": "sapSession.StartTransaction",
      "paramKey": "Tcode",
      "next": "waitVariant"
    },
    "waitVariant": {
      "action": "waitFor",
      "target": "variant.window",
      "timeout": "30s",
      "next": "setConsignment"
    },
    "setConsignment": {
      "action": "set",
      "target": "{{usr}}chkPA_XKONS",
      "paramKey": "Consignment",
      "next": "setPipeline"
    },
    "setPipeline": {
      "action": "set",
      "target": "{{usr}}chkPA_XPIPE",
      "paramKey": "Pipeline",
      "next": "setWithdrawalsNotSettled"
    },
    "setWithdrawalsNotSettled": {
      "action": "set",
      "target": "{{usr}}chkPA_XNINV",
      "paramKey": "WithdrawalsNotSettled",
      "next": "setSettledWithdrawals"
    },
    "setSettledWithdrawals": {
      "action": "set",
      "target": "{{usr}}chkPA_XINV",
      "paramKey": "SettledWithdrawals",
      "next": "setLayout"
    },
    "setLayout": {
      "action": "set",
      "target": "{{usr}}ctxtPA_VARIA",
      "paramKey": "Layout",
      "next": "setCompanyCodeLow"
    },
    "setCompanyCodeLow": {
      "action": "set",
      "target": "{{usr}}ctxtSO_BUKRS-LOW",
      "paramKey": "CompanyCodeLow",
      "next": "setCompanyCodeHigh"
    },
    "setCompanyCodeHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSO_BUKRS-HIGH",
      "paramKey": "CompanyCodeHigh",
      "next": "setVendorLow"
    },
    "setVendorLow": {
      "action": "set",
      "target": "{{usr}}ctxtSO_LIFNR-LOW",
      "paramKey": "VendorLow",
      "next": "setVendorHigh"
    },
    "setVendorHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSO_LIFNR-HIGH",
      "paramKey": "VendorHigh",
      "next": "setPlantLow"
    },
    "setPlantLow": {
      "action": "set",
      "target": "{{usr}}ctxtSO_WERKS-LOW",
      "paramKey": "PlantLow",
      "next": "setPlantHigh"
    },
    "setPlantHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSO_WERKS-HIGH",
      "paramKey": "PlantHigh",
      "next": "setMaterialLow"
    },
    "setMaterialLow": {
      "action": "set",
      "target": "{{usr}}ctxtSO_MATNR-LOW",
      "paramKey": "MaterialLow",
      "next": "setMaterialHigh"
    },
    "setMaterialHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSO_MATNR-HIGH",
      "paramKey": "MaterialHigh",
      "next": "setDocumentDateLow"
    },
    "setDocumentDateLow": {
      "action": "set",
      "target": "{{usr}}ctxtSO_BLDAT-LOW",
      "paramKey": "DocumentDateLow",
      "next": "setDocumentDateHigh"
    },
    "setDocumentDateHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSO_BLDAT-HIGH",
      "paramKey": "DocumentDateHigh",
      "next": "setPostingDateLow"
    },
    "setPostingDateLow": {
      "action": "set",
      "target": "{{usr}}ctxtSO_BUDAT-LOW",
      "paramKey": "PostingDateLow",
      "next": "setPostingDateHigh"
    },
    "setPostingDateHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSO_BUDAT-HIGH",
      "paramKey": "PostingDateHigh",
      "next": "setMaterialDocumentLow"
    },
    "setMaterialDocumentLow": {
      "action": "set",
      "target": "{{usr}}ctxtSO_MBLNR-LOW",
      "paramKey": "MaterialDocumentLow",
      "next": "setMaterialDocumentHigh"
    },
    "setMaterialDocumentHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSO_MBLNR-HIGH",
      "paramKey": "MaterialDocumentHigh",
      "next": "clickExecute"
    },
    "clickExecute": {
      "action": "click",
      "target": "variant.execBtn",
      "next": "waitProcessing"
    },
    "waitProcessing": {
      "action": "waitFor",
      "target": "processing",
      "timeout": "60s",
      "next": "checkError"
    },
    "checkError": {
      "action": "condition",
      "target": "errorDialog",
      "operator": "exists",
      "true": "notifyErr",
      "false": "download"
    },
    "notifyErr": {
      "action": "notify",
      "paramKey": "ErrorMessage",
      "next": "end"
    },
    "download": {
      "action": "click",
      "target": "saveAs.btn",
      "next": "end"
    },
    "end": {
      "action": "exit"
    }
  }
}