{
  "steps": {
    "startTransaction": {
      "action": "callProgram",
      "method": "sapSession.StartTransaction",
      "paramKey": "Tcode",
      "next": "waitVariant"
    },
    "waitVariant": {
      "action": "waitFor",
      "target": "variant.window",
      "timeout": "30s",
      "next": "setScopeOfList"
    },
    "setScopeOfList": {
      "action": "set",
      "target": "me2n.filter.scopeOfList",
      "paramKey": "ScopeofList",
      "next": "setValidityKeyDate"
    },
    "setValidityKeyDate": {
