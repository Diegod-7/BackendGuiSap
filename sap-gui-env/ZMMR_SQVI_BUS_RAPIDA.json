{
  "steps": {
    "startTransaction": {
      "action": "callProgram",
      "method": "sapSession.StartTransaction",
      "paramKey": "Tcode",
      "next": "waitVariant"
    },
    "waitVariant": {
      "action": "waitFor",
      "target": "variant.window",
      "timeout": "30s",
      "next": "setLayout"
    },
    "setLayout": {
      "action": "set",
      "target": "{{usr}}ctxt%LAYOUT",
      "paramKey": "Layout",
      "next": "setDGProfLow"
    },
    "setDGProfLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00011-LOW",
      "paramKey": "DangerousGoodsProfLow",
      "next": "setDGProfHigh"
    },
    "setDGProfHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00011-HIGH",
      "paramKey": "DangerousGoodsProfHigh",
      "next": "setPlantLow"
    },
    "setPlantLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00002-LOW",
      "paramKey": "PlantLow",
      "next": "setPlantHigh"
    },
    "setPlantHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00002-HIGH",
      "paramKey": "PlantHigh",
      "next": "setMatLow"
    },
    "setMatLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00003-LOW",
      "paramKey": "MaterialLow",
      "next": "setMatHigh"
    },
    "setMatHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00003-HIGH",
      "paramKey": "MaterialHigh",
      "next": "setValClassLow"
    },
    "setValClassLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00010-LOW",
      "paramKey": "ValuationClassLow",
      "next": "setValClassHigh"
    },
    "setValClassHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00010-HIGH",
      "paramKey": "ValuationClassHigh",
      "next": "setLangLow"
    },
    "setLangLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00005-LOW",
      "paramKey": "LanguageKeyLow",
      "next": "setLangHigh"
    },
    "setLangHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00005-HIGH",
      "paramKey": "LanguageKeyHigh",
      "next": "setMRPTLow"
    },
    "setMRPTLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00006-LOW",
      "paramKey": "MRPTypeLow",
      "next": "setMRPTHigh"
    },
    "setMRPTHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00006-HIGH",
      "paramKey": "MRPTypeHigh",
      "next": "setMatGrpLow"
    },
    "setMatGrpLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00007-LOW",
      "paramKey": "MaterialGroupLow",
      "next": "setMatGrpHigh"
    },
    "setMatGrpHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00007-HIGH",
      "paramKey": "MaterialGroupHigh",
      "next": "setPurchGrpLow"
    },
    "setPurchGrpLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00008-LOW",
      "paramKey": "PurchasingGroupLow",
      "next": "setPurchGrpHigh"
    },
    "setPurchGrpHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00008-HIGH",
      "paramKey": "PurchasingGroupHigh",
      "next": "setMRPCtrlLow"
    },
    "setMRPCtrlLow": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00009-LOW",
      "paramKey": "MRPControllerLow",
      "next": "setMRPCtrlHigh"
    },
    "setMRPCtrlHigh": {
      "action": "set",
      "target": "{{usr}}ctxtSP$00009-HIGH",
      "paramKey": "MRPControllerHigh",
      "next": "clickExecute"
    },
    "clickExecute": {
      "action": "click",
      "target": "variant.execBtn",
      "next": "waitProcessing"
    },
    "waitProcessing": {
      "action": "waitFor",
      "target": "processing",
      "timeout": "60s",
      "next": "checkError"
    },
    "checkError": {
      "action": "condition",
      "target": "errorDialog",
      "operator": "exists",
      "true": "notifyErr",
      "false": "download"
    },
    "notifyErr": {
      "action": "notify",
      "paramKey": "ErrorMessage",
      "next": "end"
    },
    "download": {
      "action": "click",
      "target": "saveAs.btn",
      "next": "end"
    },
    "end": {
      "action": "exit"
    }
  }
}